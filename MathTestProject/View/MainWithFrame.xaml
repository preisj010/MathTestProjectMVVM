<Window x:Class="MathTestProject.View.MainWithFrame"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MathTestProject.View"
        mc:Ignorable="d"
        Title="MainWithFrame" Height="710" Width="1600">
    <Grid>

        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <Menu Grid.Row="0">

            <MenuItem Header="Search" Name="searchMenu" />
            <MenuItem Header="Logout" Name="logoutMenu" Click="ClickLogout"/>
            <MenuItem Header="Exit" Name="exitMenu" Click="ClickClose"/>
        </Menu>
        <Frame x:Name="mainFrame" NavigationUIVisibility="Hidden" Grid.Row="1"/>

    </Grid>
</Window>